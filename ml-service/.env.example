# TytoAlba ML Service Configuration

# API Settings
API_HOST=0.0.0.0
API_PORT=8000

# CORS Settings (comma-separated origins)
CORS_ORIGINS=*

# Model Path
MODEL_PATH=models/vessel_arrival_lstm.h5

# Database Configuration (for AIS data)
# Uncomment and configure when ready to connect to AIS database
# DB_HOST=localhost
# DB_PORT=5432
# DB_NAME=ais_database
# DB_USER=your_username
# DB_PASSWORD=your_password

# Weather API Configuration
# Uncomment and add your API key when ready
# WEATHER_API_KEY=your_openweather_api_key
# WEATHER_API_URL=https://api.openweathermap.org/data/2.5/

# Logging
LOG_LEVEL=INFO
